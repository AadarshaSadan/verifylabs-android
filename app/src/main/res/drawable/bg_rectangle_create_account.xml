<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">

    <!-- Default state -->
    <item>
        <shape android:shape="rectangle">
            <!-- Background color with 20% opacity (ARGB: 33 = 20%) -->
            <solid android:color="#1922376D"/> <!-- Replace 2196F3 with your desired color -->
            <corners android:radius="5dp"/> <!-- 5dp corner radius -->
        </shape>
    </item>

</selector>
